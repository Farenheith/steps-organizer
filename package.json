{
  "name": "steps-organizer",
  "version": "0.0.1",
  "description": "Steps Organizer",
  "main": "index.js",
  "scripts": {
    "test": "nyc npm run testNoCoverage",
    "testNoCoverage": "jasmine-ts --config=jasmine.json",
    "build": "tsc -d"
  },
  "author": "Farenheith",
  "license": "MIT",
  "dependencies": {
    "@types/joi": "^14.3.3",
    "@types/uuid": "^3.4.4",
    "base-ddd": "github:Farenheith/base-ddd",
    "bluebird": "^3.5.4",
    "inversify": "^5.0.1",
    "joi": "^14.3.1",
    "node-ts": "^5.0.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^7.0.0",
    "typescript": "^3.4.4",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.26",
    "@types/jasmine": "^3.3.12",
    "jasmine": "^3.4.0",
    "jasmine-console-reporter": "^3.1.0",
    "jasmine-ts": "^0.3.0",
    "nyc": "^14.0.0"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "src/interfaces",
      "src/index.ts",
      "coverage",
      "test",
      "build"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "all": true,
    "check-coverage": true,
    "lines": 100,
    "branches": 100,
    "functions": 100
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Farenheith/steps-organizer.git"
  },
  "keywords": [
    "steps",
    "organizer"
  ],
  "bugs": {
    "url": "https://github.com/Farenheith/steps-organizer/issues"
  },
  "homepage": "https://github.com/Farenheith/steps-organizer#readme"
}
